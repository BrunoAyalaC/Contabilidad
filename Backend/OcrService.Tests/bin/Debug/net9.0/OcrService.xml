<?xml version="1.0"?>
<doc>
    <assembly>
        <name>OcrService</name>
    </assembly>
    <members>
        <member name="T:OcrService.Models.OcrJob">
            <summary>
            Represents an OCR processing job for a single uploaded invoice PDF.
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.Status">
            <summary>
            Job status: Pending, Processing, Completed, Failed.
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.CreatedAt">
            <summary>
            UTC timestamp when the job was created.
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.StartedAt">
            <summary>
            UTC timestamp when processing started (nullable).
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.CompletedAt">
            <summary>
            UTC timestamp when processing completed (nullable).
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.ErrorMessage">
            <summary>
            Optional error message when the job fails.
            </summary>
        </member>
        <member name="P:OcrService.Models.OcrJob.Confidence">
            <summary>
            Overall confidence score (0..1) for the parsed data.
            </summary>
        </member>
        <member name="T:OcrService.Models.ParsedInvoiceData">
            <summary>
            Structured representation of an extracted invoice. Serialized into <see cref="P:OcrService.Models.OcrJob.ParsedData"/>.
            </summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.InvoiceNumber">
            <summary>Invoice identifier extracted from the document (when available).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.InvoiceDate">
            <summary>Invoice date if parsable.</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.Ruc">
            <summary>RUC (tax identifier) if found.</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.PartyName">
            <summary>Supplier or customer name when detected.</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.TotalAmount">
            <summary>Total amount parsed from the invoice.</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.TaxAmount">
            <summary>Tax amount parsed (IGV/IVA).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.Currency">
            <summary>Currency code (e.g., PEN, USD).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.DocumentType">
            <summary>Document classification (FACTURA, BOLETA, etc.).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.LineItems">
            <summary>Extracted line items (if any).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceData.ConfidenceScores">
            <summary>
            Confidence per field (0..1).
            </summary>
        </member>
        <member name="T:OcrService.Models.ParsedInvoiceLine">
            <summary>
            Line item extracted from an invoice.
            </summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceLine.Description">
            <summary>Item description or product name.</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceLine.Quantity">
            <summary>Quantity parsed (nullable).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceLine.UnitPrice">
            <summary>Unit price parsed (nullable).</summary>
        </member>
        <member name="P:OcrService.Models.ParsedInvoiceLine.Total">
            <summary>Total line amount.</summary>
        </member>
    </members>
</doc>
